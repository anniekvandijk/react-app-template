{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "import",                                                                                                              
    "react",                                                                                                               
    "jsx-a11y" 
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "linebreak-style": "off",
    "comma-dangle": ["error", "never"],
    "id-length": 0,
    "indent": [1, 2],
    "no-console": 0,
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "strict": 0
  }
}