{
  "name": "react-app-template",
  "version": "1.0.0",
  "description": "A template for react with Webpack and Babel",
  "author": "Anniek van Dijk, Animundo",
  "repository": {
    "type": "git",
    "url": "https://github.com/anniekvandijk/react-app-template"
  },
  "license": "ISC",
  "main": "src/client/index.js",
  "scripts": {
    "start:dev:client": "webpack-dev-server --mode development --config ./config/webpack.base.config.js --hot --inline",
    "start:dev:server": "nodemon src/server/start.js",
    "start:dev": "concurrently \"npm run start:dev:server\" \"npm run start:dev:client\"",
    "prestart:prod": "webpack --mode production --config config/webpack.prd.config.js",
    "start:prd:server": "node src/server/start.js"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  }
}
